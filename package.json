{
  "name": "vue-autocomplete-list",
  "version": "1.0.0",
  "main": "dist/vue-autocomplete-list.umd.js",
  "module": "dist/vue-autocomplete-list.esm.js",
  "unpkg": "dist/vue-autocomplete-list.min.js",
  "browser": {
    "./sfc": "src/vue-autocomplete-list.vue"
  },
  "author": {
    "name": "Guillaume Perche",
    "email": "hello@iamgpe.me"
  },
  "license": "MIT",
  "scripts": {
    "test": "vue serve --open ./src/test/test.vue",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vue-autocomplete-list.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/vue-autocomplete-list.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vue-autocomplete-list.min.js"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@vue/compiler-sfc": "^3.0.2",
    "rollup": "^2.33.2",
    "rollup-plugin-css-bundle": "^1.0.4",
    "rollup-plugin-vue": "^6.0.0-beta.11",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12"
  },
  "dependencies": {
    "autoprefixer": "^10.0.2",
    "postcss": "^8.1.7"
  }
}
